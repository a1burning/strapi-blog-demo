    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "author": {
      "type": "json"
    },
    "content": {
      "type": "richtext"
    },
    "timeToRead": {
      "type": "string"
    }
  }
}
